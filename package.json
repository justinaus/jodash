{
  "name": "jodash",
  "version": "1.1.8",
  "private": false,
  "main": "dist/index.js",
  "repository": "https://github.com/justinaus/jodash.git",
  "author": "justin.koo <justin.koo@kakaocorp.com>",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html",
    "build": "concurrently \"npm:prebuild\" \"parcel build index.ts\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "develop": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "prebuild": "rm -rf dist"
  },
  "devDependencies": {
    "src": "^1.1.2",
    "concurrently": "^5.0.1",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.7.3"
  }
}
